{
    "name": "innis-maggiore/silverstripe-smtp-tester",
    "description": "Silverstripe SMTP Tester allows developers to test their site's SMTP configuration from the admin area.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/innis-maggiore/silverstripe-smtp-tester",
    "keywords": ["silverstripe", "smtp"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dave Collins",
            "email": "web-dev@innismaggiore.com"
        }, {
            "name": "Joe Chenevey",
            "email": "web-dev@innismaggiore.com"
        }
    ],
    "support": {
        "issues": "https://github.com/innis-maggiore/silverstripe-smtp-tester/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "silverstripe/cms": "^4.2 || ^5.0 || ^6.0"
    },
    "replace": {
        "creativecodelabs/silverstripe-smtp-tester": "*"
    },
    "autoload": {
        "psr-4": {
            "InnisMaggiore\\SilverstripeSMTPTester\\": "src/"
        }
    },
    "extra": {
        "installer-name": "silverstripe-smtp-tester",
        "expose": [
            "css",
            "images"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
